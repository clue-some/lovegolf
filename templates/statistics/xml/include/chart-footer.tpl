<styles>
    <definition>
        <style name='ogtCaptionFont' type='font' font='Arial' size='20' color='222222'/>
        <style name='ogtAxisFont' type='font' font='Arial' size='14' color='222222'/>
        <style name='ogtValuesFont' type='font' font='Arial' size='13' color='222222'/>
        <style name='ogtToopTipFont' type='font' font='Arial' size='14' color='222222'/>
